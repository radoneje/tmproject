    .videoWr.flex.between
            each sect in commands
                .section
                    .sectionWr
                        each item in geografy[sect.id]
                            .geografyText #{item.title}
                            .geografyWr
                                each record in item.items
                                    if record.isActive
                                        .geografyItem.flex
                                            .geografyItemDecorWr
                                                .geografyItemDecor
                                            .geografyItemText #{record.title}




